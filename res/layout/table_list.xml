<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/tbl_ViewGroup" android:layout_width="match_parent"
	android:layout_height="match_parent" android:orientation="vertical">
	<RelativeLayout android:background="@drawable/titlebar"
		android:layout_width="match_parent" android:id="@+id/linearLayout1"
		android:layout_height="44dp" android:orientation="horizontal"
		android:baselineAligned="false">
		<Button android:layout_centerVertical="true"
			android:layout_alignParentLeft="true" android:layout_width="30dp"
			android:layout_marginLeft="17dp" android:background="@drawable/nav_back"
			android:id="@+id/btnBack" android:layout_height="31dp"></Button>
		<TextView android:layout_alignParentRight="true"
			android:layout_centerVertical="true" android:textSize="15dp"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="wrap_content" android:id="@+id/txtDate_title"
			android:layout_height="wrap_content" android:text="          "></TextView>
		<TextView android:textAppearance="?android:attr/textAppearanceLarge"
			android:layout_width="wrap_content" android:id="@+id/txtTitle"
			android:layout_height="wrap_content" android:layout_centerVertical="true"
			android:layout_centerHorizontal="true" android:text="Dentists"></TextView>
	</RelativeLayout>
	<LinearLayout android:layout_width="match_parent"
		android:id="@+id/linearLayout_1" android:orientation="vertical"
		android:background="@drawable/background" android:weightSum="1"
		android:layout_height="match_parent">
		<ScrollView android:id="@+id/scrollView2"
			android:layout_width="match_parent" android:layout_height="279dp"
			android:layout_weight="1.04">
			<LinearLayout android:id="@+id/layout_appointments"
				android:layout_width="match_parent" android:orientation="vertical"
				android:layout_height="23dp"></LinearLayout>
		</ScrollView>
	</LinearLayout>
</LinearLayout>
