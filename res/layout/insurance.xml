<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ins_ViewGroup"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/RelativeLayout01"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@drawable/titlebar" >

        <Button
            android:id="@+id/btnBack"
            android:layout_width="30dp"
            android:layout_height="31dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="17dp"
            android:background="@drawable/nav_back" >
        </Button>

        <TextView
            android:id="@+id/txtDate_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="           "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="View Insurance"
            android:textAppearance="?android:attr/textAppearanceLarge" >
        </TextView>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background" >

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="17dp"
            android:layout_marginTop="18dp"
            android:text="Member Name :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtMember"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/TextView03"
            android:layout_marginLeft="19dp"
            android:layout_toRightOf="@+id/TextView03"
            android:text="                   "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView03"
            android:layout_below="@+id/TextView03"
            android:layout_marginTop="14dp"
            android:text="Insurance Company Name :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtCompany"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView01"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView01"
            android:layout_marginLeft="36dp"
            android:text="             "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView01"
            android:layout_below="@+id/txtCompany"
            android:text="Address :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView06"
            android:layout_below="@+id/TextView06"
            android:text="City :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView07"
            android:layout_below="@+id/TextView07"
            android:text="State :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView08"
            android:layout_below="@+id/TextView08"
            android:text="ZIP Code :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView09"
            android:layout_below="@+id/TextView09"
            android:layout_marginTop="18dp"
            android:text="Phone/Office :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView11"
            android:layout_below="@+id/TextView11"
            android:layout_marginTop="21dp"
            android:text="Member ID :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView12"
            android:layout_below="@+id/TextView12"
            android:layout_marginTop="16dp"
            android:text="Group # :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/TextView05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView12"
            android:layout_below="@+id/TextView12"
            android:layout_marginTop="53dp"
            android:text="BIN # :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txtMember"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/txtCompany"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtCity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txtMember"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TextView06"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtState"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txtCity"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/txtCity"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtZip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txtState"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/txtState"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtPhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/TextView10"
            android:layout_toLeftOf="@+id/btnPhone"
            android:layout_toRightOf="@+id/TextView03"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtPolicy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txtPhone"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/TextView11"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txtPolicy"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/TextView12"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtGroup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txtID"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/TextView13"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <TextView
            android:id="@+id/txtBin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txtID"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/TextView05"
            android:text="               "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <Button
            android:id="@+id/btnHome"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_below="@+id/txtBin"
            android:layout_marginTop="18dp"
            android:layout_toRightOf="@+id/TextView09"
            android:background="@drawable/button"
            android:text="Home"
            android:textColor="@color/white" >
        </Button>

        <TextView
            android:id="@+id/TextView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/TextView10"
            android:layout_below="@+id/TextView10"
            android:text="Policy Number :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="15dp" >
        </TextView>

        <Button
            android:id="@+id/btnPhone"
            android:layout_marginTop="-10dp"
	        android:layout_width="32dp"
	        android:layout_height="32dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/txtPhone"
            android:layout_marginRight="26dp"
            android:background="@drawable/tel" >
        </Button>
    </RelativeLayout>

</LinearLayout>