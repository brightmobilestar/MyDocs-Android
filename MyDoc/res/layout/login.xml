<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/lg_ViewGroup" android:orientation="vertical"
	android:layout_width="match_parent" android:layout_height="match_parent">
	<ScrollView android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/scrollView1">
		<LinearLayout android:id="@+id/linearLayout1"
			android:layout_width="match_parent" android:orientation="vertical" android:layout_height="match_parent">
			<LinearLayout android:orientation="vertical"
				android:layout_width="match_parent" android:id="@+id/linearLayout_1"
				android:layout_height="44dp" android:background="@drawable/titlebar1">
			</LinearLayout>
			<RelativeLayout android:background="@drawable/background"
				android:id="@+id/relativeLayout1" android:layout_width="fill_parent" android:layout_height="524dp">
				<Button android:id="@+id/btnLogin" android:background="@drawable/button"
					android:text="Login" android:layout_width="100dp"
					android:layout_height="30dp" android:layout_alignTop="@+id/btnCancel1"
					android:layout_alignLeft="@+id/medication9name"
					android:layout_marginLeft="16dp" android:textColor="@color/white"></Button>
				<TextView android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="User Name" android:layout_width="wrap_content"
					android:id="@+id/TextView01" android:layout_height="wrap_content"
					android:layout_above="@+id/editPSW" android:layout_alignParentLeft="true"
					android:layout_marginLeft="37dp" android:layout_marginBottom="41dp"></TextView>
				<EditText android:layout_width="wrap_content" android:id="@+id/password"
					android:layout_height="40dp" android:layout_alignTop="@+id/TextView01"
					android:layout_alignParentRight="true" android:layout_toRightOf="@+id/btnLogin" android:textSize="14dp">
					<requestFocus></requestFocus>
				</EditText>
				<EditText android:layout_width="wrap_content"
					android:inputType="textPassword" android:id="@+id/editPSW"
					android:layout_height="40dp" android:layout_alignTop="@+id/medication9name"
					android:layout_toRightOf="@+id/btnLogin"
					android:layout_alignParentRight="true" android:textSize="14dp"></EditText>
				<TextView android:textAppearance="?android:attr/textAppearanceMedium"
					android:text="Password" android:layout_width="wrap_content"
					android:id="@+id/medication9name" android:layout_height="wrap_content"
					android:layout_above="@+id/btnLogin" android:layout_alignLeft="@+id/TextView01"
					android:layout_marginBottom="48dp"></TextView>

				<Button
				    android:id="@+id/btnCancel1"
				    android:layout_width="100dp"
				    android:layout_height="30dp"
				    android:layout_alignLeft="@+id/editPSW"
				    android:layout_alignParentTop="true"
				    android:layout_marginLeft="37dp"
				    android:layout_marginTop="223dp"
				    android:background="@drawable/button"
				    android:text="Cancel"
				    android:textColor="@color/white" />

			</RelativeLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
